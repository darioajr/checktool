<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository/>
  <interactiveMode/>
  <usePluginRegistry/>
  <offline/>
  <pluginGroups/>
  <servers>
    <server> 
      <id>maven.oracle.com </id> 
      <username>${env.CI_ORACLE_USERNAME}</username> 
      <password>${env.CI_ORACLE_PASSWORD}</password> 
       <configuration>
         <basicAuthScope>
           <host>ANY </host>
           <port>ANY </port>
           <realm>OAM 11g </realm>
         </basicAuthScope>
         <httpConfiguration>
            <all>
              <params>
                <property>
                  <name>http.protocol.allow-circular-redirects </name>
                  <value>%b,true </value>
                </property>
              </params>
            </all>
         </httpConfiguration>
       </configuration>
    </server> 
    <server>
      <id>sonatype-nexus-snapshots</id>
      <username>${env.CI_DEPLOY_USERNAME}</username>
      <password>${env.CI_DEPLOY_PASSWORD}</password>
    </server>
  </servers>
  <mirrors/>
  <proxies/>
  <profiles/>
  <activeProfiles/>
</settings>